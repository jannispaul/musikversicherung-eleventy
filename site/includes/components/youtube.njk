{% macro youtube(videoId, title) %}
	<div class="iframe-container mb-x1">
		<iframe title={{ title }} src="https://www.youtube-nocookie.com/embed/{{ videoId }}?rel=0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen class="w-full"/>
	</div>
{% endmacro %}
